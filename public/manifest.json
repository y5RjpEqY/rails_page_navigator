{
    "name": "Rails Page Navigator",
    "description": "Rails Page Navigator",
    "version": "1.0.0",
    "manifest_version": 3,

    "action": {
      "default_popup": "popup.html"
    },

    "options_ui": {
      "page": "options.html"
    },

    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+H",
                "mac": "MacCtrl+Shift+H"
            },
            "description": "Run extension."
        }
    },

    "permissions": [
      "storage"
    ],

    "incognito": "split"
}