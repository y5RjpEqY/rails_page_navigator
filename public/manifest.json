{
    "name": "My Extension",
    "description": "My Extension",
    "version": "1.0.0",
    "manifest_version": 3,

    "action": {
      "default_popup": "popup.html"
    },

    "options_ui": {
      "page": "options.html"
    },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["js/vendor.js"]
      }
    ],

    "permissions": [
      "storage"
    ],

    "host_permissions": [
      "<all_urls>"
    ]
}